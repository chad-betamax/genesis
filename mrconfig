[$HOME/.config/vcsh/repo.d/bash.git]
checkout = GIT_SSH_COMMAND="ssh -i ~/.ssh/id_github -oStrictHostKeyChecking=no -oIdentitiesOnly=yes" vcsh clone --branch bash git@github.com:chad-betamax/configs.git bash

[$HOME/.config/vcsh/repo.d/mate.git]
checkout = GIT_SSH_COMMAND="ssh -i ~/.ssh/id_github -oStrictHostKeyChecking=no -oIdentitiesOnly=yes" vcsh clone --branch mate git@github.com:chad-betamax/configs.git mate

[$HOME/.config/vcsh/repo.d/vim.git]
checkout = GIT_SSH_COMMAND="ssh -i ~/.ssh/id_github -oStrictHostKeyChecking=no -oIdentitiesOnly=yes" vcsh clone --branch vim git@github.com:chad-betamax/configs.git vim

[$HOME/.config/vcsh/repo.d/ssh.git]
checkout = GIT_SSH_COMMAND="ssh -i ~/.ssh/id_github -oStrictHostKeyChecking=no -oIdentitiesOnly=yes" vcsh clone --branch ssh git@github.com:chad-betamax/configs.git ssh

